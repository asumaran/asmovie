meta {
  name: Get Current User
  type: http
  seq: 3
}

get {
  url: {{host}}/auth/me
  body: none
  auth: bearer
}

auth:bearer {
  token: {{jwt_token}}
}

tests {
  test("should get current user successfully", function() {
    expect(res.getStatus()).to.equal(200);
    expect(res.getBody()).to.have.property('id');
    expect(res.getBody()).to.have.property('email');
    expect(res.getBody()).to.not.have.property('password');
  });
}
